desc:{"type":"field","size":[400,60],"pos":[56,60],"locked":1,"value":"Release histamine and mediators in allergic responses and inflammation."}
markers:{"type":"field","size":[400,56],"pos":[56,130],"locked":1,"value":"Markers: \n\nGeneral: CD18, CD31, CD116, FceRI, IL2R, IL8R\n\nActivated: CD63. CD123, CD203c, CCR3, LAMP1"}
