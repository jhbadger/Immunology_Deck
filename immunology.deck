{card:T cell}
{widgets}
title:{"type":"field","size":[400,30],"pos":[56,20],"locked":1,"value":"T cell"}
desc:{"type":"field","size":[400,60],"pos":[56,60],"locked":1,"value":"T cells (CD4+ helper, CD8+ cytotoxic) are central to adaptive immunity. CD4+ cells coordinate immune responses; CD8+ cells kill infected or cancerous cells."}
markers:{"type":"field","size":[400,20],"pos":[56,130],"locked":1,"value":"Markers: CD3, CD4 (helper), CD8 (cytotoxic)"}
origin:{"type":"field","size":[400,20],"pos":[56,155],"locked":1,"value":"Origin: Bone marrow (precursor), matures in thymus"}
btn_back:{"type":"button","size":[150,24],"pos":[56,200],"script":"T cell.0","text":"Back to Immune Cells"}

{script:T cell.0}
on click do
  go["immune_cells"]
end
{end}

{card:B cell}
{widgets}
title:{"type":"field","size":[400,30],"pos":[56,20],"locked":1,"value":"B cell"}
desc:{"type":"field","size":[400,60],"pos":[56,60],"locked":1,"value":"B cells produce antibodies to neutralize pathogens. Can become plasma or memory cells."}
markers:{"type":"field","size":[400,20],"pos":[56,130],"locked":1,"value":"Markers: CD19, CD20, CD21, CD22, CD79a"}
origin:{"type":"field","size":[400,20],"pos":[56,155],"locked":1,"value":"Origin: Bone marrow"}
btn_back:{"type":"button","size":[150,24],"pos":[56,200],"script":"T cell.0","text":"Back to Immune Cells"}

{card:NK cell}
{widgets}
title:{"type":"field","size":[400,30],"pos":[56,20],"locked":1,"value":"Natural Killer (NK) cell"}
desc:{"type":"field","size":[400,60],"pos":[56,60],"locked":1,"value":"NK cells are innate lymphocytes that kill virus-infected and tumor cells without prior sensitization."}
markers:{"type":"field","size":[400,20],"pos":[56,130],"locked":1,"value":"Markers: CD16, CD56 (no CD3)"}
origin:{"type":"field","size":[400,20],"pos":[56,155],"locked":1,"value":"Origin: Bone marrow"}
btn_back:{"type":"button","size":[150,24],"pos":[56,200],"script":"T cell.0","text":"Back to Immune Cells"}

{card:Dendritic cell}
{widgets}
title:{"type":"field","size":[400,30],"pos":[56,20],"locked":1,"value":"Dendritic cell"}
desc:{"type":"field","size":[400,60],"pos":[56,60],"locked":1,"value":"Antigen-presenting cells that activate T cells and initiate adaptive immunity."}
markers:{"type":"field","size":[400,20],"pos":[56,130],"locked":1,"value":"Markers: CD11c, MHC II, CD80, CD86"}
origin:{"type":"field","size":[400,20],"pos":[56,155],"locked":1,"value":"Origin: Bone marrow"}
btn_back:{"type":"button","size":[150,24],"pos":[56,200],"script":"T cell.0","text":"Back to Immune Cells"}

{card:Macrophage}
{widgets}
title:{"type":"field","size":[400,30],"pos":[56,20],"locked":1,"value":"Macrophage"}
desc:{"type":"field","size":[400,60],"pos":[56,60],"locked":1,"value":"Large phagocytes that engulf pathogens, present antigens, and produce cytokines."}
markers:{"type":"field","size":[400,20],"pos":[56,130],"locked":1,"value":"Markers: CD14, CD68, MHC II"}
origin:{"type":"field","size":[400,20],"pos":[56,155],"locked":1,"value":"Origin: Bone marrow (monocytes in blood, mature in tissues)"}
btn_back:{"type":"button","size":[150,24],"pos":[56,200],"script":"T cell.0","text":"Back to Immune Cells"}

{card:Neutrophil}
{widgets}
title:{"type":"field","size":[400,30],"pos":[56,20],"locked":1,"value":"Neutrophil"}
desc:{"type":"field","size":[400,60],"pos":[56,60],"locked":1,"value":"Most abundant white blood cells, first responders to infection, strong phagocytes."}
markers:{"type":"field","size":[400,20],"pos":[56,130],"locked":1,"value":"Markers: CD15, CD16, CD66b"}
origin:{"type":"field","size":[400,20],"pos":[56,155],"locked":1,"value":"Origin: Bone marrow"}
btn_back:{"type":"button","size":[150,24],"pos":[56,200],"script":"T cell.0","text":"Back to Immune Cells"}

{card:Eosinophil}
{widgets}
title:{"type":"field","size":[400,30],"pos":[56,20],"locked":1,"value":"Eosinophil"}
desc:{"type":"field","size":[400,60],"pos":[56,60],"locked":1,"value":"Defend against parasites and involved in allergy and asthma."}
markers:{"type":"field","size":[400,20],"pos":[56,130],"locked":1,"value":"Markers: CD9, CD125, Siglec-8"}
origin:{"type":"field","size":[400,20],"pos":[56,155],"locked":1,"value":"Origin: Bone marrow"}
btn_back:{"type":"button","size":[150,24],"pos":[56,200],"script":"T cell.0","text":"Back to Immune Cells"}

{card:Basophil}
{widgets}
title:{"type":"field","size":[400,30],"pos":[56,20],"locked":1,"value":"Basophil"}
desc:{"type":"field","size":[400,60],"pos":[56,60],"locked":1,"value":"Release histamine and mediators in allergic responses and inflammation."}
markers:{"type":"field","size":[400,20],"pos":[56,130],"locked":1,"value":"Markers: CD123, CD203c, Fc�RI"}
origin:{"type":"field","size":[400,20],"pos":[56,155],"locked":1,"value":"Origin: Bone marrow"}
btn_back:{"type":"button","size":[150,24],"pos":[56,200],"script":"T cell.0","text":"Back to Immune Cells"}

{card:Mast cell}
{widgets}
title:{"type":"field","size":[400,30],"pos":[56,20],"locked":1,"value":"Mast cell"}
desc:{"type":"field","size":[400,60],"pos":[56,60],"locked":1,"value":"Release histamine and other mediators; key effector in allergic reactions."}
markers:{"type":"field","size":[400,20],"pos":[56,130],"locked":1,"value":"Markers: CD117 (c-Kit), Fc�RI, tryptase"}
origin:{"type":"field","size":[400,20],"pos":[56,155],"locked":1,"value":"Origin: Bone marrow (mature in tissues)"}
btn_back:{"type":"button","size":[150,24],"pos":[56,200],"script":"T cell.0","text":"Back to Immune Cells"}

{card:Monocyte}
{widgets}
title:{"type":"field","size":[400,30],"pos":[56,20],"locked":1,"value":"Monocyte"}
desc:{"type":"field","size":[400,60],"pos":[56,60],"locked":1,"value":"Circulating precursors to macrophages and dendritic cells; phagocytic."}
markers:{"type":"field","size":[400,20],"pos":[56,130],"locked":1,"value":"Markers: CD14, CD16"}
origin:{"type":"field","size":[400,20],"pos":[56,155],"locked":1,"value":"Origin: Bone marrow"}
btn_back:{"type":"button","size":[150,24],"pos":[56,200],"script":"T cell.0","text":"Back to Immune Cells"}

{card:Plasma cell}
{widgets}
title:{"type":"field","size":[400,30],"pos":[56,20],"locked":1,"value":"Plasma cell"}
desc:{"type":"field","size":[400,60],"pos":[56,60],"locked":1,"value":"Terminally differentiated B cells that secrete large amounts of antibody."}
markers:{"type":"field","size":[400,20],"pos":[56,130],"locked":1,"value":"Markers: CD38, CD138, loss of CD19"}
origin:{"type":"field","size":[400,20],"pos":[56,155],"locked":1,"value":"Origin: Bone marrow (from B cells)"}
btn_back:{"type":"button","size":[150,24],"pos":[56,200],"script":"T cell.0","text":"Back to Immune Cells"}

{card:Memory cell}
{widgets}
title:{"type":"field","size":[400,30],"pos":[56,20],"locked":1,"value":"Memory cell"}
desc:{"type":"field","size":[400,60],"pos":[56,60],"locked":1,"value":"Long-lived lymphocytes (T or B) that provide rapid, robust responses upon re-exposure to antigen."}
markers:{"type":"field","size":[400,20],"pos":[56,130],"locked":1,"value":"Markers: CD27, CD45RO (T); CD27, CD19 (B)"}
origin:{"type":"field","size":[400,20],"pos":[56,155],"locked":1,"value":"Origin: Derived from activated T or B cells"}
